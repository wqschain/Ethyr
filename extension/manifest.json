{
  "manifest_version": 3,
  "name": "Ethyr",
  "version": "1.0.0",
  "description": "AI-powered Ethereum address analyzer for fraud detection",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [{
    "resources": ["icons/*"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 